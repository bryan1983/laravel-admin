{
    "name": "joselfonseca/laravel-admin",
    "description": "Admin panel for laravel 5 apps. work in progress!",
    "keywords": [
        "laravel-admin",
        "laravel Administrator",
        "ACL",
        "CMS"
    ],
    "homepage": "https://github.com/joselfonseca/laravel-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Jose Luis Fonseca",
            "email": "jose@ditecnologia.com",
            "homepage": "https://josefonseca.me",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git://github.com/orchestral/phpseclib.git"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "zizaco/entrust": "1.6.*",
        "laravelcollective/html": "~5.1",
        "rcrowe/twigbridge" : "0.7.*",
        "laracasts/flash": "~1.3",
        "barryvdh/laravel-debugbar": "^2.0@dev",
        "uxweb/sweet-alert": "~1.1",
        "laracasts/utilities": "~2.0"
    },
    "require-dev": {
        "orchestra/testbench": "3.1.*",
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Joselfonseca\\LaravelAdmin\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joselfonseca\\LaravelAdmin\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
